resource_name = "maker_details"
script/source = "extends Panel

var _player: Player = Global.player
var _last_selected: int = 0

@onready var text_error: Label = $VBox/Error
@onready var input_name: LineEdit = $VBox/FieldName/Text
@onready var option_genre: OptionButton = $VBox/FieldGenre/Genre

var view_list: Panel
var gamelist: VBoxContainer
var gamelist_item: Panel


func _ready():
	view_list = get_node(\"../List\")
	gamelist = view_list.get_node(\"ScrollContainer/Games\")
	gamelist_item = gamelist.get_node(\"GameItem0\")


func _close():
	visible = false
	view_list.visible = true
	_clear()


func _clear():
	text_error.text = \"\"
	input_name.clear()
	option_genre.selected = -1


func _on_make_done_pressed():
	var game_name: String = input_name.text

	if not game_name:
		text_error.text = \"Please, type game name!\"
		return

	if option_genre.selected < 0:
		text_error.text = \"Please, select game genre!\"
		return

	# Append new game to player object
	_player.add_game(game_name, option_genre.selected)

	_last_selected = option_genre.selected

	# Append also on ListGames
	var gameitem: Panel = gamelist_item.duplicate()
	gameitem.name = \"GameItem{i}\".format({\"i\": _player.game_counter})
	gameitem.visible = true
	gameitem.get_node(\"Name\").text = game_name
	gamelist.add_child(gameitem)

	_close()


func _on_visibility_changed():
	if visible:
		# Fill genre options
		option_genre.clear()
		option_genre.add_item(\"Free\")

		if _player.skills.able_ads:
			option_genre.add_item(\"Free with Ads\")

		if _player.skills.able_microtransactions:
			option_genre.add_item(\"Free with Microtransactions\")

		if _player.skills.able_p2w:
			option_genre.add_item(\"Free with Pay-to-Win\")

		if _player.skills.able_monetize:
			option_genre.add_item(\"Free with Monetization\")

		if _player.skills.able_paid:
			option_genre.add_item(\"Paid\")

		option_genre.selected = _last_selected
	else:
		_clear()


func _on_make_cancel_pressed():
	_close()
"

