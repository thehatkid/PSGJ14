resource_name = "window_maker"
script/source = "extends CanvasLayer

var _player: Player = Global.player

@onready var progressbar: ProgressBar = $Window/Container/List/Buttons/Make/ProgressBar
@onready var warn_text: Label = $Window/Container/List/Warning
@onready var gamelist: VBoxContainer = $Window/Container/List/ScrollContainer/Games
@onready var btn_make: Button = $Window/Container/List/Buttons/Make


func _ready():
	progressbar.visible = false
	warn_text.visible = false


func _process(_delta: float):
	var last_game = _player.get_last_game()

	if _player.energy < 20:
		warn_text.visible = true
	else:
		warn_text.visible = false

	if last_game:
		# Update \"Make a Game\" button in Make window
		btn_make.get_node(\"ProgressBar\").value = last_game.finished_at
		if last_game.finished:
			btn_make.disabled = false
			btn_make.get_node(\"ProgressBar\").visible = false
		else:
			btn_make.disabled = true
			btn_make.get_node(\"ProgressBar\").visible = true

	# Update game items in games list
	for game in _player.games:
		var item: Panel = gamelist.get_node_or_null(\"GameItem{i}\".format({\"i\": game.id}))
		if item:
			var state_text: String
			if game.finished:
				state_text = \"Downloads: {dc}\".format({\"dc\": game.downloads})
			else:
				state_text = \"Making, finished at {v}%...\".format({\"v\": round(game.finished_at)})
			item.get_node(\"State\").text = state_text


func _on_close_gui_input(event: InputEvent):
	if event is InputEventMouseButton:
		if event.pressed and event.button_index == 1:
			if self.visible:
				self.visible = false


func _on_make_pressed():
	$Window/Container/Details._clear()
	$Window/Container/List.visible = false
	$Window/Container/Details.visible = true
"

