[sub_resource type="GDScript" id="GDScript_ihpjx"]
resource_name = "layer_new_player"
script/source = "extends CanvasLayer

var player: Player = Global.player

@onready var input_player_name: LineEdit = $VBox/FieldName/Text
@onready var text_error: Label = $VBox/Error
@onready var btn_done: Button = $VBox/Buttons/Done


func _ready():
	pass


func _clear():
	input_player_name.clear()
	text_error.text = \"\"


func _on_name_text_changed(new_text: String):
	if not new_text or len(new_text) < 2:
		btn_done.disabled = true
	else:
		btn_done.disabled = false


func _on_done_pressed():
	var player_name: String = input_player_name.text

	if not player_name:
		text_error.text = \"Please, type player name!\"
		return

	# TODO: special names handling, just for fun :)

	# Set out values
	player.player_name = player_name
	player.set_day_paused(false)

	# Clean up
	self.visible = false
	self._clear()
"

