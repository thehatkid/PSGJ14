[sub_resource type="GDScript" id="GDScript_r8u00"]
resource_name = "taskbar"
script/source = "extends Panel

var _player: Player = Global.player

var startmenu: Panel
var time_text: Label


func _ready():
	startmenu = get_node(\"../StartMenu\")
	time_text = $Right/Time


func _process(_delta):
	time_text.text = \"{time}\\nDay {day}\".format({
		\"time\": _player.get_day_time_string(),
		\"day\": _player.get_day_count()
	})

	if _player.havent_sleep:
		time_text.modulate = Color(1.0, 0.6, 0.6)
	elif _player.day.time >= 1320:
		time_text.modulate = Color(1.0, 0.7, 0.7)
	elif _player.day.time >= 1200:
		time_text.modulate = Color(1.0, 0.8, 0.8)
	else:
		time_text.modulate = Color(1.0, 1.0, 1.0)


func _on_start_pressed():
	if not startmenu.visible:
		startmenu.visible = true
	else:
		startmenu.visible = false
"

