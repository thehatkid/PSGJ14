resource_name = "window_skills"
script/source = "extends CanvasLayer

var _player: Player = Global.player

var _skills: Dictionary = {}
var _boosters: Dictionary = {}

var _boosters_costs: Dictionary = {
	\"speed\": 100,
	\"exp\": 250,
	\"downloads\": 400
}

@onready var _title: Label = $Window/Container/Content/Title
@onready var _list_skills: VBoxContainer = $Window/Container/Content/TabContainer/Skills/ScrollContainer/Skills
@onready var _list_boosters: VBoxContainer = $Window/Container/Content/TabContainer/Boosters/ScrollContainer/Boosters
@onready var _se_learned: AudioStreamPlayer = $SELearned


func _ready():
	# Get all skills
	_skills[\"1\"] = _list_skills.get_node(\"Skill1/VBox/Claim\")
	_skills[\"2\"] = _list_skills.get_node(\"Skill2/VBox/Claim\")
	_skills[\"3\"] = _list_skills.get_node(\"Skill3/VBox/Claim\")
	_skills[\"4\"] = _list_skills.get_node(\"Skill4/VBox/Claim\")
	_skills[\"5\"] = _list_skills.get_node(\"Skill5/VBox/Claim\")

	# Get all boosters
	_boosters[\"1\"] = _list_boosters.get_node(\"Boost1\")
	_boosters[\"2\"] = _list_boosters.get_node(\"Boost2\")
	_boosters[\"3\"] = _list_boosters.get_node(\"Boost3\")


func _process(_delta: float):
	# Update title text
	_title.text = \"You have {e} experience and ${m} money.\".format({
		\"e\": _player.experience,
		\"m\": _player.money
	})

#region Update skills buttons
	if _player.experience >= 2 and not _player.skills.able_ads:
		_skills[\"1\"].disabled = false
	if _player.experience >= 8 and not _player.skills.able_microtransactions:
		_skills[\"2\"].disabled = false
	if _player.experience >= 16 and not _player.skills.able_p2w:
		_skills[\"3\"].disabled = false
	if _player.experience >= 25 and not _player.skills.able_monetize:
		_skills[\"4\"].disabled = false
	if _player.experience >= 40 and not _player.skills.able_paid:
		_skills[\"5\"].disabled = false

	if _player.skills.able_ads:
		_skills[\"1\"].text = \"Learned!\"
		_skills[\"1\"].disabled = true
	if _player.skills.able_microtransactions:
		_skills[\"2\"].text = \"Learned!\"
		_skills[\"2\"].disabled = true
	if _player.skills.able_p2w:
		_skills[\"3\"].text = \"Learned!\"
		_skills[\"3\"].disabled = true
	if _player.skills.able_monetize:
		_skills[\"4\"].text = \"Learned!\"
		_skills[\"4\"].disabled = true
	if _player.skills.able_paid:
		_skills[\"5\"].text = \"Learned!\"
		_skills[\"5\"].disabled = true
#endregion

#region Update boosters
	# Get all nodes of boosters
	var boost_1_title: Label = _boosters[\"1\"].get_node(\"Title\")
	var boost_2_title: Label = _boosters[\"2\"].get_node(\"Title\")
	var boost_3_title: Label = _boosters[\"3\"].get_node(\"Title\")
	var boost_1_btn: Button = _boosters[\"1\"].get_node(\"VBox/Buy\")
	var boost_2_btn: Button = _boosters[\"2\"].get_node(\"VBox/Buy\")
	var boost_3_btn: Button = _boosters[\"3\"].get_node(\"VBox/Buy\")
	var boost_1_cost: Label = _boosters[\"1\"].get_node(\"VBox/Cost\")
	var boost_2_cost: Label = _boosters[\"2\"].get_node(\"VBox/Cost\")
	var boost_3_cost: Label = _boosters[\"3\"].get_node(\"VBox/Cost\")

	# Set boosters texts
	boost_1_title.text = \"Boost Game Development {n}\".format({\"n\": _player.boosters.speed + 1})
	boost_2_title.text = \"Boost Experience Gain {n}\".format({\"n\": _player.boosters.exp + 1})
	boost_3_title.text = \"Boost Download Counts {n}\".format({\"n\": _player.boosters.downloads + 1})

	# Set boosters cost texts
	boost_1_cost.text = \"Cost: ${n}\".format({
		\"n\": _boosters_costs.speed * (_player.boosters.speed + 1)
	})
	boost_2_cost.text = \"Cost: ${n}\".format({
		\"n\": _boosters_costs.exp * (_player.boosters.exp + 1)
	})
	boost_3_cost.text = \"Cost: ${n}\".format({
		\"n\": _boosters_costs.downloads * (_player.boosters.downloads + 1)
	})

	# Unlock buttons when certain money is enough to buy
	if _player.money >= _boosters_costs.speed * (_player.boosters.speed + 1):
		boost_1_btn.disabled = false
	else:
		boost_1_btn.disabled = true

	if _player.money >= _boosters_costs.exp * (_player.boosters.exp + 1):
		boost_2_btn.disabled = false
	else:
		boost_2_btn.disabled = true

	if _player.money >= _boosters_costs.downloads * (_player.boosters.downloads + 1):
		boost_3_btn.disabled = false
	else:
		boost_3_btn.disabled = true

	# Limit boosters amount
	if _player.boosters.speed >= 9:
		boost_1_title.text = \"Boost Game Development {n}\".format({\"n\": _player.boosters.speed})
		boost_1_btn.text = \"MAX\"
		boost_1_btn.disabled = true

	if _player.boosters.exp >= 5:
		boost_2_title.text = \"Boost Experience Gain {n}\".format({\"n\": _player.boosters.exp})
		boost_2_btn.text = \"MAX\"
		boost_2_btn.disabled = true
#endregion


func _on_close_gui_input(event: InputEvent):
	if event is InputEventMouseButton:
		if event.pressed and event.button_index == 1:
			if self.visible:
				self.visible = false


#region All skills buttons pressed signals
func _on_skill1_claim_pressed():
	_player.skills.able_ads = true
	_se_learned.play()


func _on_skill2_claim_pressed():
	_player.skills.able_microtransactions = true
	_se_learned.play()


func _on_skill3_claim_pressed():
	_player.skills.able_p2w = true
	_se_learned.play()


func _on_skill4_claim_pressed():
	_player.skills.able_monetize = true
	_se_learned.play()


func _on_skill5_claim_pressed():
	_player.skills.able_paid = true
	_se_learned.play()
#endregion


#region All boosters buttons pressed signals
func _on_booster1_buy_pressed():
	_player.money -= _boosters_costs.speed * (_player.boosters.speed + 1)
	_player.boosters.speed += 1
	_se_learned.play()


func _on_booster2_buy_pressed():
	_player.money -= _boosters_costs.exp * (_player.boosters.exp + 1)
	_player.boosters.exp += 1
	_se_learned.play()


func _on_booster3_buy_pressed():
	_player.money -= _boosters_costs.downloads * (_player.boosters.downloads + 1)
	_player.boosters.downloads += 1
	_se_learned.play()
#endregion
"

