[sub_resource type="GDScript" id="GDScript_drg8n"]
resource_name = "layer_day"
script/source = "extends CanvasLayer

var _player: Player = Global.player
var _timer: float = 0.0

@onready var _bg: ColorRect = $Background
@onready var _text: Label = $CenterContainer/Text


func _process(delta: float):
	if visible:
		_timer += delta

		if _timer >= 2.0:
			# Fade in
			_bg.modulate.a -= delta
			_text.modulate.a -= delta

			if _bg.modulate.a <= 0.0:
				visible = false
				_player.set_day_paused(false)


func _on_visibility_changed():
	if visible:
		if _player.get_day_count() >= 8:
			get_node(\"../../..\")._end_game()

		# Update text and color
		_text.text = \"Day {d}\".format({\"d\": _player.get_day_count()})

		match _player.get_day_count():
			5:
				_text.modulate = Color(1.0, 0.8, 0.8)
			6:
				_text.modulate = Color(1.0, 0.6, 0.6)
			7:
				_text.modulate = Color(1.0, 0.4, 0.4)
			_:
				_text.modulate = Color(1.0, 1.0, 1.0)
	else:
		# Reset to defaults
		_bg.modulate.a = 1.0
		_text.modulate.a = 1.0
		_timer = 0.0
"

