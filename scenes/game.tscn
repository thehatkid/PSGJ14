resource_name = "debugtext"
script/source = "extends RichTextLabel

var _player: Player = Global.player


func _ready():
	if Global.debug:
		visible = true
	else:
		queue_free()
 

func _process(_delta):
	if not Global.debug:
		return

	text = \"Day: {d} | Time: {t} | Haven't sleep: {s} | Paused: {p}\\n\".format({
		\"d\": _player.get_day_count(),
		\"t\": _player.get_day_time_string(),
		\"s\": _player.havent_sleep,
		\"p\": _player.is_day_paused()
	})
	text += \"Energy: {e} | Mental Health: {h}\\n\".format({
		\"e\": _player.energy,
		\"h\": _player.mental_health
	})
	text += \"Experience: {e} | Money: {m} | Energy consume: {ec}\\n\".format({
		\"e\": _player.experience,
		\"m\": _player.money,
		\"ec\": _player.get_energy_consuming()
	})
	text += \"Downloads multiply: {dlm} | Experience multiply: {em}\\n\".format({
		\"dlm\": _player.get_downloads_multiply(),
		\"em\": _player.get_experience_multiply()
	})
	text += \"Skills: {s}\\n\".format({\"s\": _player.skills})
	text += \"Boosters: {b}\\n\".format({\"b\": _player.boosters})
	text += \"Total download count: {n}\".format({\"n\": _player.get_total_download_count()})

	if _player.games:
		text += \"\\nGames:\\n\\n\"
		for g in _player.games:
			text += \"#{i}: {n} ({at}%) | Genre: {g}\\n\".format({
				\"i\": g.id, \"n\": g.name, \"at\": g.finished_at, \"g\": g.genre
			})
			text += \"  Downloads: {d} | Rating: {r} | Paid off: {p}\\n\".format({
				\"d\": g.downloads, \"r\": g.rating, \"p\": g.paid_off
			})
			text += \"\\n\"
"

